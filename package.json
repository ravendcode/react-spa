  "scripts": {
    "start": "node ./dist/server/bundle.js",
    "dev": "concurrently \"npm run client-dev\" \"npm run server-dev\"",
    "prod": "concurrently \"npm run client-prod\" \"npm run server-prod\"",
    "server-dev": "export DEBUG=app:* || set DEBUG=app:* && nodemon ./src/server/index.js -e js,hbs --watch ./src/server",
    "server-prod": "export NODE_ENV=production || set NODE_ENV=production&& gulp clean-server && webpack --config webpack.server.config.js && gulp locales",
    "client-dev": "export DEBUG=app:* || set DEBUG=app:* && webpack-dev-server --inline --history-api-fallback",
    "client-prod": "export NODE_ENV=production || set NODE_ENV=production&& gulp clean-client && webpack && gulp",
    "test": "export NODE_ENV=test || set NODE_ENV=test&& nodemon --exec mocha src/server/**/*.test.js --compilers js:babel-core/register --require babel-polyfill"
  },
